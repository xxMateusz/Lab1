@model Contact
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@if (Model.Count > 0)
{

    <table class="table">
        <thead>
            <tr>
                <th>
                    Imie
                </th>
                <th>
                    Email
                </th>
                <th>
                    Numer tel
                </th>
                <th>
                   data urodzenia
                </th>
                <th>
                    Akcje
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td class="@(item.Value.Name.EndsWith("a")? "text-primary":"text-secondary")">
                        @item.Value.Name
                    </td>
                    <td>
                        @item.Value.Email
                    </td>
                    <td>
                        @item.Value.Phone
                    </td>
                    <td>
                        @item.Value.Birth
                    </td>
                   
                </tr>
            }
        </tbody>
    </table>
}